@{
}

<table border="1">
    
    <tr>
        <th> Product Id </th>
        <th> Product Name </th>
        <th> Supplier Id </th>
        <th> Category Id </th>
        <th> Unit Price </th>
    </tr>
    @foreach (var p in ViewBag.ListOfProducts)
    {
        <tr>
            <td> @p.ProductId </td>
            <td> @p.ProductName </td>
            <td> @p.SupplierId </td>
            <td> @p.CategoryId </td>
            <td> @p.UnitPrice </td>
        </tr>
    }

</table>

